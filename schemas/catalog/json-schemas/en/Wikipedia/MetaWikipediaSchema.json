{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "[[Sources]]"
    },
    "format": {
      "type": "string",
      "const": "[[Encyclopedia Entries]]"
    },
    "areas": {
      "type": "array",
      "items": {
        "type": "string",
        "const": "[[Knowledge]]"
      }
    },
    "color": {
      "type": "string",
      "const": "#3171B2"
    },
    "icon": {
      "default": "wikipedia",
      "description": "Lucide icon.",
      "type": "string"
    },
    "banner": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "description": "URL of the article"
    },
    "platform": {
      "type": "string",
      "const": "Wikipedia"
    }
  },
  "required": [
    "type",
    "format",
    "areas",
    "color",
    "url",
    "platform"
  ],
  "description": "Wikipedia entry: Metadata of Note"
}