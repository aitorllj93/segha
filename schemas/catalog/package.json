{
  "name": "@segha/catalog",
  "version": "5.0.2",
  "main": "index.ts",
  "description": "Schemas for Petroglyph Catalog",
  "segha": {
    "i18n": true
  },
  "scripts": {
    "docs": "pnpm run docs:root && pnpm run docs:en && pnpm run docs:es",
    "docs:root": "pnpm tsx ../../utils/generate-docs.ts --entry ./index.ts --output ./README.md --cwd .",
    "docs:en": "pnpm tsx ../../utils/generate-docs.ts --entry ./en/index.ts --output ./en/README.md --cwd .",
    "docs:es": "pnpm tsx ../../utils/generate-docs.ts --entry ./es/index.ts --output ./es/README.md --cwd .",
    "json-schema": "pnpm tsx ../../utils/generate-json-schema.ts --entry ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "schemas",
    "zod",
    "obsidianmd"
  ],
  "author": "Aitor Llamas Jim√©nez <aitorllj93@gmail.com>",
  "homepage": "https://github.com/aitorllj93/segha",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/aitorllj93"
    },
    {
      "type": "ko_fi",
      "url": "https://ko-fi.com/aitorllj93"
    },
    {
      "type": "buy_me_a_coffee",
      "url": "https://buymeacoffee.com/aitorllj93"
    },
    {
      "type": "individual",
      "url": "https://www.paypal.com/paypalme/aitorllj93"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aitorllj93/segha.git",
    "directory": "schemas/catalog"
  },
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./en": "./en/index.ts",
    "./en/types": "./en/types.ts",
    "./en/json-schema": "./json-schemas/en/index.json",
    "./en/json-schemas/*": "./json-schemas/en/*.json",
    "./en/clothing": "./en/Clothing/index.ts",
    "./en/clothing/types": "./en/Clothing/types.ts",
    "./en/clothing/json-schema": "./json-schemas/en/Clothing/index.json",
    "./en/clothing/json-schemas/*": "./json-schemas/en/Clothing/*.json",
    "./es": "./es/index.ts",
    "./es/types": "./es/types.ts",
    "./es/json-schema": "./json-schemas/es/index.json",
    "./es/json-schemas/*": "./json-schemas/es/*.json",
    "./es/clothing": "./es/Clothing/index.ts",
    "./es/clothing/types": "./es/Clothing/types.ts",
    "./es/clothing/json-schema": "./json-schemas/es/Clothing/index.json",
    "./es/clothing/json-schemas/*": "./json-schemas/es/Clothing/*.json",
    "./types": "./types.ts"
  },
  "peerDependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "tsx": "catalog:",
    "zod": "catalog:",
    "zod2md": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "*.ts",
    "**/*.ts",
    "json-schemas/**/*.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "arkregex": "^0.0.5"
  }
}
