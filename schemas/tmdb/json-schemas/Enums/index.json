{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "@segha/tmdb/Enums",
  "description": "Schemas for TMDB",
  "$defs": {
    "MonetizationType": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "string",
      "enum": [
        "flatrate",
        "free",
        "ads",
        "rent",
        "buy"
      ]
    },
    "ReleaseType": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "string",
      "enum": [
        "1",
        "2",
        "3",
        "4",
        "5",
        "6"
      ]
    },
    "WatchProvider": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "number",
      "enum": [
        8,
        337,
        350,
        119,
        63,
        62,
        1899,
        188,
        541,
        538,
        283
      ]
    }
  }
}